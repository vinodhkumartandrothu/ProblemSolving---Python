        "def d_ipaddress(s,l):\n",
        "  defangIP = \"\"\n",
        "  for i in range(l):\n",
        "    if s[i] == \".\":\n",
        "      defangIP += '[.]'\n",
        "    else:\n",
        "      defangIP += s[i]\n",
        "   #else:\n",
        "      #continue\n",
        "  return defangIP\n",
        "  \n",
        "\n",
        "s = \"1.1.1.1\"\n",
        "#print(len(s))\n",
        "l = len(s)\n",
        "d_ipaddress(s, l)"
      ]
    }
