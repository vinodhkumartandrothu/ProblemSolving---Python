        "def palindrome(n):\n",
        "  rev = 0\n",
        "  temp = n\n",
        "  while(temp != 0):\n",
        "    rev = (rev*10)+ (temp % 10)\n",
        "    #print(rev)\n",
        "    temp = temp // 10\n",
        "  if (rev == n):\n",
        "    return True\n",
        "  else: \n",
        "    return False\n",
        "\n",
        "n = 121\n",
        "palindrome(n)"
      ]
    }
