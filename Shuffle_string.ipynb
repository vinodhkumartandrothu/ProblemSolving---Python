      "source": [
        "def is_shuffle_string(s, indices):\n",
        "  res = list(s)\n",
        "  l = len(s)\n",
        "  for i in range(l):\n",
        "    res[indices[i]] = s[i]\n",
        "  return ''.join(res)\n",
        "#print(res)\n",
        "\n",
        "s = \"art\"\n",
        "indices = [1,0,2]\n",
        "is_shuffle_string(s, indices)\n"
      ]
    }
