      "source": [
        "\n",
        "def reverse(s):\n",
        "    str = \"\"\n",
        "    for i in s:\n",
        "        str = i + str\n",
        "    return str\n",
        " \n",
        "s = \"Geeksforgeeks\"\n",
        " \n",
        "print(\"The original string is : \", end=\"\")\n",
        "print(s)\n",
        " \n",
        "print(\"The reversed string is : \", end=\"\")\n",
        "print(reverse(s))"
      ]
