        "def is_Final_val_var(oper):\n",
        "  x = 0\n",
        "  for i in oper:\n",
        "    if i == \"--x\":\n",
        "      x = x -1\n",
        "    elif i == \"++x\":\n",
        "      x = x + 1\n",
        "    else:\n",
        "      x = x + 1\n",
        "  return x\n",
        "\n",
        "\n",
        "oper = [\"--x\", \"++x\", \"x++\"]\n",
        "oper = [\"++X\",\"++X\",\"X++\"]\n",
        "is_Final_val_var(oper)"
