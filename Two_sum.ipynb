      "source": [
        "def two_sum(num, target):\n",
        "  sum = 0\n",
        "  res = 0\n",
        "  for i in range(len(num)-1):\n",
        "    for j in range(i+1, len(num)):\n",
        "      sum = num[i]+num[j]\n",
        "      #print(sum)\n",
        "      res = sum\n",
        "      if res == target:\n",
        "        return [i,j]\n",
        "\n",
        "num = [3,4,2]\n",
        "target = 6\n",
        "two_sum(num, target)"
      ]
    }
